<h1>Products</h1>

<div class="productsContainer">
        {{#each products}}
                <div class="product">
                        <p>{{this.title}}</p>
                        <img src={{this.thumbnail}}>
                        <p>${{this.price}}</p>
                        <p>{{this.description}}</p>
                </div>
        {{/each}}
</div>


<script src="/socket.io/socket.io.js"></script>
<script src="/js/homeViews.js"></script>